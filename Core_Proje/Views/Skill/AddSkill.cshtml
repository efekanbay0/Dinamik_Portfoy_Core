@model EntityLayer.Concrete.Skill
@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description"> Yeni Deneyim Ekleme Paneli </p>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label>Baslik</label>
                    @Html.TextBoxFor(x => x.Title , new {@class="form-control"})
                </div>
                <div class="form-group">
                    <label>Deger</label>
                    @Html.TextBoxFor(x => x.Value , new {@class="form-control"})
                </div>
                <button type="submit" class="btn btn-primary mr-2">Kaydet</button>
                @Html.ActionLink("İptal", "Index", "Skill", null, new { @class = "btn btn-danger" })
            </form>
        </div>
    </div>
</div>

